<html class="no-js" lang="en">
<!--<![endif]-->

<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>Web Delivery 1.0</title>
    <meta name="description" content="Sufee Admin - HTML5 Admin Template">
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <%-include('include/top-links-dt.ejs')%>
</head>

<body>
  <!-- Left Panel -->

	<%-include('include/left-panel.ejs')%>

	<!-- Left Panel -->
	
	<!-- Right Panel -->

	<div id="right-panel" class="right-panel">

		<%-include('include/header.ejs')%>
			<!-- Header-->

			<div class="content mt-3">
				<div class="animated fadeIn" id="variableContent">
					<div class="row">

						<div class="col-md-12">
							<div class="card">
								<div class="card-header">
									<h3 class="card-title text-center"><%=title%></h3>
								</div>
								<div class="card-body">
									<table id="bootstrap-data-table-export" class="table table-bordered table-hover">
										<thead>
											<tr>
                        <th title="id fo pedido">#</th>
												<th>Data/Hora</th>												
											</tr>
										</thead>
										<tbody>	
											<% orders.forEach(order => { %>
												<tr>
                          <td>
                            <a href="#" onclick="Order.orderDetail('<%=order.id%>', '<%=order.user%>')">
                              <%=order.id%>
                            </a>
                          </td>
													<td>
                            <a href="#" onclick="Order.orderDetail('<%=order.id%>', '<%=order.user%>')"
                              <% if(order.status == 0) { %>
                                class="no-fulfilled"
                                title="Pedido n√£o atendido"
                              <% } else { %>
                                class="fulfilled"
                                title="Pedido atendido"
                              <% } %> >
															<%=order._datetime.toLocaleString('br')%>
														</a>
													</td>													
												</tr>
											<% }) %>		
										</tbody>
									</table>
								</div>
							</div>
						</div>

					</div>
				</div><!-- .animated -->
			</div><!-- .content -->


	</div><!-- /#right-panel -->

	<!-- Right Panel -->


	<%-include('include/bottom-links-dt.ejs')%>
	<script src="/admin/admin.js"></script>

</body>

</html>
