<div class="card">
  <div class="card-header text-center"><h3>Escolha um entregador</h3></div>
  <div class="card-body card-block">
    <ul class="list-group">
      <form action="/deliveryman-choosed" method="POST" id="chooseDmForm">
      <%for(deliveryman of deliverymens) { %>
      <li class="list-group-item">
        <input type="radio" name="deliverymanChoosed" value="<%=deliveryman.id%>"> <%=deliveryman.name%>
      </li>
      <% } %>
      </form>
    </ul>
  </div>
  <div class="card-footer">
    <button class="btn btn-info" 
      onclick="Deliveryman.chooseDeliveryman();">Enviar</button>
  </div>
</div>
