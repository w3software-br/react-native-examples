<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="description" content="Sufee Admin - HTML5 Admin Template">
    <meta name="viewport" content="width=device-width, initial-scale=1">
		<title>Web Delivery 1.0</title>
    <%-include('include/top-links-dt.ejs')%>
</head>

<body>
    <!-- Left Panel -->

	<%-include('include/left-panel.ejs')%>

	<!-- Left Panel -->

	<!-- Right Panel -->

	<div id="right-panel" class="right-panel">

		<%-include('include/header.ejs')%>
			<!-- Header-->

			<div class="content mt-3">
				<div class="animated fadeIn" id="variableContent">
					<div class="row">

						<div class="col-md-12">
							<div class="card">
								<div class="card-header">
									<h3 class="card-title text-center">Todas os Bairros</h3>
								</div>
								<div class="card-body">
									<table id="bootstrap-data-table-export" class="table table-striped table-bordered">
										<thead>
											<tr>
												<th>Name</th>
											</tr>
										</thead>
										<tbody>
											<% neighborhoods.forEach(neighborhood => { %>
												<tr>
													<td>
														<a href="#"
															onclick="Neighborhood.formEditNeighborhood('<%=neighborhood.id%>')">
															<%=neighborhood.name%>
													</td>
                        </a>
												</tr>
											<% }) %>
										</tbody>
									</table>
								</div>
							</div>
						</div>

					</div>
				</div><!-- .animated -->
			</div><!-- .content -->


	</div><!-- /#right-panel -->

	<!-- Right Panel -->


	<%-include('include/bottom-links-dt.ejs')%>
	<script src="/admin/admin.js"></script>

</body>

</html>
