<div class="card">
  <div class="card-header text-center"><h3>Editar Categoria</h3></div>
  <div class="card-body card-block">
    <form action="/edit-category" method="post" id="categoryForm">
      <input type="hidden" name="id" value="<%=category.id%>">
      <div class="form-group">
        <div class="input-group">
          <div class="input-group-addon">Nome</div>
          <input type="text" id="name" name="name" 
            placeholder="Nome da categorira" class="form-control" 
            name="name" value="<%=category.name%>">
        </div>
      </div>

      <div class="form-actions form-group">
        <div class="row">
          <div class="col-sm-3">
            <button type="button" class="form-control btn btn-info"
              onclick="Category.editCategory()">Editar</button>
          </div>      
        </div>        
      </div>
      
    </form>
  </div>
</div>




